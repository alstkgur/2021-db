<h1>{{ title }}</h1>
<table>
  <tr>
    <td>airplane_id</td>
    <td>total_number_of_seats</td>
    <td>airplane_type</td>
  </tr>
  {{#each airplane}}
    <form method="post">
      <tr>
        <td>{{airplane_id}}</td>
        <td>{{total_number_of_seats}}</td>  
        <td>{{airplane_type}}</td>  
        <td style="border: none; margin-left: 10px;">
          <button  
            style="margin-left: 10px;"
            type="submit" 
            name="delBtn"
            value= "{{Name}}"
            formaction="/delete"
            >
          삭제
          </button>
        </td>
      </tr>    
    </form>
  {{/each}}
</table>